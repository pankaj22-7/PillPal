{
  "expo": {
    "name": "PillPal",
    "slug": "PillPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tester11111.PillPal",
      "infoPlist": {
        "UIBackgroundModes": ["background-processing"],
        "NSUserNotificationAlertStyle": "banner"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tester11111.PillPal",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "USE_FULL_SCREEN_INTENT",
        "SYSTEM_ALERT_WINDOW"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b07cee24-a029-4bee-a35b-6853935a3e2e"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#4CAF50",
          "defaultChannel": "default"
        }
      ],
      "expo-dev-client"
    ]
  }
}
